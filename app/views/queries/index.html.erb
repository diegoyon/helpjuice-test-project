<div class="container">
  <h1>Most searched</h1>

  <table class="table table-hover table-striped">
    <thead class="table-dark">
      <tr>
        <th>Query</th>
        <th>Instances</th>
      </tr>
    </thead>
    <tbody>
      <% @queries.each do |query| %>
        <tr>
          <th><%= query[0] %></th>
          <th><%= query[1] %></th>
        </tr>
      <% end %>
    </tbody>
  </table>
  
</div>